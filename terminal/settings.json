{
  "$help": "https://aka.ms/terminal-documentation",
  "$schema": "https://aka.ms/terminal-profiles-schema",
  "actions": [
    {
      "command": {
        "action": "copy",
        "singleLine": false
      },
      "keys": "ctrl+c"
    },
    {
      "command": {
        "action": "newTab"
      },
      "keys": "ctrl+t"
    },
    {
      "command": "paste",
      "keys": "ctrl+v"
    },
    {
      "command": {
        "action": "splitPane",
        "split": "up",
        "splitMode": "duplicate"
      },
      "keys": "ctrl+minus"
    },
    {
      "command": "find",
      "keys": "ctrl+f"
    },
    {
      "command": {
        "action": "splitPane",
        "split": "auto",
        "splitMode": "duplicate"
      },
      "keys": "ctrl+d"
    },
    {
      "command": "closePane",
      "keys": "ctrl+w"
    },
    {
      "command": {
        "action": "splitPane",
        "split": "right",
        "splitMode": "duplicate"
      },
      "keys": "ctrl+\\"
    },
    {
      "command": {
        "action": "globalSummon"
      },
      "keys": "win+esc"
    },
    {
      "command": "unbound",
      "keys": "f9"
    }
  ],
  "alwaysShowNotificationIcon": true,
  "autoHideWindow": false,
  "centerOnLaunch": false,
  "confirmCloseAllTabs": true,
  "copyFormatting": "none",
  "copyOnSelect": false,
  "defaultProfile": "{574e775e-4f2a-5b96-ac1e-a2962a402336}",
  "firstWindowPreference": "persistedWindowLayout",
  "initialCols": 120,
  "initialPosition": ",",
  "initialRows": 30,
  "launchMode": "default",
  "minimizeToNotificationArea": true,
  "newTabMenu": [
    {
      "type": "remainingProfiles"
    }
  ],
  "newTabPosition": "afterCurrentTab",
